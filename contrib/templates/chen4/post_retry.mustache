<!doctype html><html><head>
  <title>try again</title>
    <link rel="stylesheet" href="{{prefix}}static/chen4.css">
</head>
<form action="{{post_url}}" enctype="multipart/form-data" name="post" method="post">
  {{{csrf}}}
  <input type="hidden" name="reference" value="{{reference}}" id="postform_reference">
  <table>
    <tr>
      <th>text</th>
      <td><textarea id="postform_message" name="message" cols=40 rows=5></textarea></td>
    </tr>
    <tr>
      <th>subject</th>
      <td><input type="text" name="subject" value=""> <input class="postform_attachment" id="postform_attachments" type="file" name="attachment_uploaded" multiple></td>
    </tr>
    <tr>
      <th>name</th>
      <td><input type="text" name="name" value=""> <input type="checkbox" name="dubs">dubs</td>
    </tr>
    <tr>
      <th>captcha</th>
      <td><input type="text" name="captcha" autocomplete="off"> <input type="submit" value="{{button}}" class="button" id="postform_submit"></td>
    </tr>
  </table>
  <img id="captcha_img" src="{{prefix}}captcha/img" alt="captcha">
</form>
